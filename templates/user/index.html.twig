<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Touch Hospital Medical Template{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="" />
    <meta name="author" content="http://webthemez.com" />
    <!-- css -->
    <link href="{{ asset('FrontOffice/css/bootstrap.min.css') }}" rel="stylesheet" /> 
    <link href="{{ asset('FrontOffice/css/fancybox/jquery.fancybox.css') }}" rel="stylesheet" />
    <link href="{{ asset('FrontOffice/css/flexslider.css') }}" rel="stylesheet" /> 
    <link href="{{ asset('FrontOffice/css/style.css') }}" rel="stylesheet" />
  
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
    {% include 'user/header.html.twig' %}
    {% block body %}{% endblock %}
    {% include 'user/footer.html.twig' %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        <script src="{{ asset('FrontOffice/js/jquery.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/jquery.easing.1.3.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/jquery.fancybox.pack.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/jquery.fancybox-media.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/portfolio/jquery.quicksand.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/portfolio/setting.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/jquery.flexslider.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/animate.js') }}"></script>
        <script src="{{ asset('FrontOffice/js/custom.js') }}"></script>
    {% endblock %}
</body>
</html>


